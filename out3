    (function(){
        
        jQuery(function(){
            
            $('aside.promoFrame').each(function(index, promo){
                promo = $(promo);
                var slot;
                var element = promo;
                var elementHeightDiff;
                while(elementHeightDiff !== 0) {
                    if (element && element.length) {
                        element = element.next();                    
                        if (element.is('.onetAdsLeftSlot')) {
                            if (slot && slot.length) {
                                break;
                            }
                            else {
                                slot = element;
                            }
                        }
                        else {
                            elementHeightDiff = element.height();
                            promo.hide();
                            elementHeightDiff -= element.height();
                            promo.show();
                            elementHeightDiff = Math.max(elementHeightDiff, 0);
                        }
                    }
                    else {
                        break;
                    }
                }
                if (slot && slot.length && element && element.length) {
                    slot.insertAfter(element);
                }
            });
            var lastAsideBottom;
            $('aside.promoFrame').each(function(index, promo){
                promo = $(promo);
                var currentTop = promo.offset().top;
                if (currentTop <= lastAsideBottom)
                {
                    var next = promo.next();
                    promo.insertAfter(next);
                }
                lastAsideBottom = promo.offset().top + promo.outerHeight();                
            });
            
            var table = findTable();
            var slots = jQuery('.onetAdsLeftSlot');
            
            if (table === null) {
                
                if (slots.length >= 1)
                    runAds(slots.eq(0));
                }
            
            else {

                if (slots.length === 1)
                    runTable(slots.eq(0), table);

                else if (slots.length >= 2) {

                    runTable(slots.eq(0), table);
                    runAds(slots.eq(1));
                    }
                }
            });
        
        function runTable(contener, table) {
            
            contener.css({
                'clear': 'left',
                'float': 'left',
                'margin': '10px 20px 10px 0'
                });
            
            var contenerWrap = createElement(contener, '<div></div>');
            contenerWrap.width(300);
            
            table = table.detach();
            table.addClass("otdv-in-content");
            contenerWrap.append(table);
            }
        
        function runAds(contener) {
            
            contener.addClass('onet-ad');
            contener.attr('id', 'onet-ad-left');
            
            onetAds.addSlot('left');
            onetAds.fetch();
            }
        
        function findTable() {
            
            var table = jQuery('.widgetMoveToContent');
            
            if (table.length > 0)
                return table.eq(0);
            
            return null;
            }
        
        function createElement(parent, newTag) {
            
            var newElement = jQuery(newTag);
            parent.append(newElement);
            return newElement;
            }
